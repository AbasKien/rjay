{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.reduce.js\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      form: {\n        contact_number: \"\",\n        address: \"\",\n        payment: \"\"\n      }\n    };\n  },\n  computed: {\n    totalItems() {\n      const cart = JSON.parse(localStorage.getItem(\"cartItems\")) || [];\n      return cart.reduce((sum, item) => sum + item.quantity, 0);\n    },\n    totalPrice() {\n      const cart = JSON.parse(localStorage.getItem(\"cartItems\")) || [];\n      return cart.reduce((total, item) => total + item.price * item.quantity, 0).toFixed(2);\n    }\n  },\n  methods: {\n    validateContactNumber() {\n      this.form.contact_number = this.form.contact_number.replace(/[^0-9]/g, \"\");\n    },\n    async checkout() {\n      try {\n        const response = await axios.post(\"http://localhost:5555/orders\", {\n          contact_number: this.form.contact_number,\n          address: this.form.address,\n          payment: this.form.payment,\n          total_price: (parseFloat(this.totalPrice) + 58).toFixed(2)\n        });\n        alert(response.data.message);\n        this.$router.push(\"/thank-you\");\n      } catch (error) {\n        console.error(\"Failed to checkout:\", error);\n        alert(\"There was an error placing your order. Please try again.\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","form","contact_number","address","payment","computed","totalItems","cart","JSON","parse","localStorage","getItem","reduce","sum","item","quantity","totalPrice","total","price","toFixed","methods","validateContactNumber","replace","checkout","response","post","total_price","parseFloat","alert","message","$router","push","error","console"],"sources":["C:\\laragon\\www\\project_appdev\\Frontend\\src\\views\\checkout.vue"],"sourcesContent":["<template>\n  <div class=\"checkout-container\">\n    <h1 class=\"checkout-title\">Checkout</h1>\n\n    <form @submit.prevent=\"checkout\" class=\"checkout-form\">\n      <!-- Personal Info Section -->\n      <div class=\"section personal-info-section\">\n        <h2 class=\"section-title\">Personal Information</h2>\n        <div class=\"form-group\">\n          <label for=\"contact-number\">Contact Number</label>\n          <input\n            type=\"text\"\n            id=\"contact-number\"\n            v-model=\"form.contact_number\"\n            required\n            placeholder=\"Enter your contact number\"\n            @input=\"validateContactNumber\"\n            maxlength=\"11\"\n          />\n        </div>\n        <div class=\"form-group\">\n          <label for=\"address\">Shipping Address</label>\n          <textarea\n            id=\"address\"\n            v-model=\"form.address\"\n            required\n            placeholder=\"Enter your shipping address\"\n          ></textarea>\n        </div>\n      </div>\n\n      <!-- Payment Section -->\n      <div class=\"section payment-info-section\">\n        <h2 class=\"section-title\">Payment Information</h2>\n        <div class=\"form-group\">\n          <label for=\"payment\">Payment Method</label>\n          <select v-model=\"form.payment\" required>\n            <option value=\"check_payment\">Check Payment</option>\n            <option value=\"cod\">Cash on Delivery (COD)</option>\n          </select>\n        </div>\n      </div>\n\n      <!-- Order Summary Section -->\n      <div class=\"section order-summary-section\">\n        <h2 class=\"section-title\">Order Summary</h2>\n        <div class=\"order-summary\">\n          <div class=\"order-item\">\n            <span>Total Items:</span>\n            <span>{{ totalItems }}</span>\n          </div>\n          <div class=\"order-item\">\n            <span>Total Price:</span>\n            <span>₱{{ totalPrice }}</span>\n          </div>\n          <div class=\"order-item\">\n            <span>Shipping:</span>\n            <span>₱58.00</span>\n          </div>\n          <div class=\"order-total\">\n            <span><strong>Total Amount:</strong></span>\n            <span><strong>₱{{ (parseFloat(totalPrice) + 58).toFixed(2) }}</strong></span>\n          </div>\n        </div>\n      </div>\n\n      <!-- Checkout Button -->\n      <div class=\"button-container\">\n        <button type=\"submit\" class=\"place-order-button\">Place Order</button>\n      </div>\n    </form>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  data() {\n    return {\n      form: {\n        contact_number: \"\",\n        address: \"\",\n        payment: \"\",\n      },\n    };\n  },\n  computed: {\n    totalItems() {\n      const cart = JSON.parse(localStorage.getItem(\"cartItems\")) || [];\n      return cart.reduce((sum, item) => sum + item.quantity, 0);\n    },\n    totalPrice() {\n      const cart = JSON.parse(localStorage.getItem(\"cartItems\")) || [];\n      return cart\n        .reduce((total, item) => total + item.price * item.quantity, 0)\n        .toFixed(2);\n    },\n  },\n  methods: {\n    validateContactNumber() {\n      this.form.contact_number = this.form.contact_number.replace(/[^0-9]/g, \"\");\n    },\n    async checkout() {\n      try {\n        const response = await axios.post(\"http://localhost:5555/orders\", {\n          contact_number: this.form.contact_number,\n          address: this.form.address,\n          payment: this.form.payment,\n          total_price: (parseFloat(this.totalPrice) + 58).toFixed(2),\n        });\n\n        alert(response.data.message);\n        this.$router.push(\"/thank-you\");\n      } catch (error) {\n        console.error(\"Failed to checkout:\", error);\n        alert(\"There was an error placing your order. Please try again.\");\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.checkout-container {\n  max-width: 900px;\n  margin: 0 auto;\n  padding: 30px;\n  background: linear-gradient(135deg, #ffffff, #f0f4f8);\n  border-radius: 15px;\n  box-shadow: 0 15px 30px rgba(0, 0, 0, 0.1);\n  font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;\n  overflow: hidden;\n}\n\nh1.checkout-title {\n  font-size: 2.5rem;\n  font-weight: 700;\n  color: #333;\n  text-align: center;\n  margin-bottom: 40px;\n}\n\n.checkout-form {\n  display: flex;\n  flex-direction: column;\n  gap: 30px;\n}\n\n.section {\n  padding: 20px;\n  border-radius: 12px;\n  background-color: #ffffff;\n  box-shadow: 0 5px 15px rgba(0, 0, 0, 0.05);\n  transition: transform 0.2s, box-shadow 0.3s;\n}\n\n.section:hover {\n  transform: translateY(-3px);\n  box-shadow: 0 10px 20px rgba(0, 0, 0, 0.1);\n}\n\n.section-title {\n  font-size: 1.5rem;\n  font-weight: 600;\n  color: #4a4a4a;\n  margin-bottom: 15px;\n}\n\n.form-group {\n  margin-bottom: 20px;\n}\n\nlabel {\n  display: block;\n  font-size: 1.1rem;\n  color: #555;\n  margin-bottom: 8px;\n}\n\ninput,\ntextarea,\nselect {\n  width: 100%;\n  padding: 12px;\n  border-radius: 8px;\n  border: 1px solid #ccc;\n  background-color: #f7f9fc;\n  font-size: 1rem;\n  margin-top: 8px;\n  transition: border-color 0.3s ease;\n}\n\ninput:focus,\ntextarea:focus,\nselect:focus {\n  border-color: #4caf50;\n  outline: none;\n  box-shadow: 0 0 5px rgba(76, 175, 80, 0.2);\n}\n\ntextarea {\n  min-height: 120px;\n  resize: vertical;\n}\n\n.order-summary {\n  font-size: 1.2rem;\n  color: #333;\n  margin-top: 20px;\n  background-color: #e9f5e9;\n  padding: 15px;\n  border-radius: 10px;\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.05);\n}\n\n.order-item {\n  display: flex;\n  justify-content: space-between;\n  margin-bottom: 15px;\n  font-weight: 500;\n}\n\n.order-total {\n  display: flex;\n  justify-content: space-between;\n  font-size: 1.2rem;\n  font-weight: bold;\n  margin-top: 15px;\n  color: #4caf50;\n}\n\n.button-container {\n  display: flex;\n  justify-content: center;\n  margin-top: 20px;\n}\n\n.place-order-button {\n  background-color: #4caf50;\n  color: white;\n  padding: 15px 25px;\n  border-radius: 10px;\n  border: none;\n  font-size: 1.2rem;\n  cursor: pointer;\n  transition: background-color 0.3s ease, transform 0.2s;\n}\n\n.place-order-button:hover {\n  background-color: #45a049;\n  transform: scale(1.05);\n}\n\n.place-order-button:active {\n  background-color: #388e3c;\n  transform: scale(1);\n}\n\n.place-order-button:disabled {\n  background-color: #a5d6a7;\n  cursor: not-allowed;\n}\n\n@media (max-width: 768px) {\n  .checkout-container {\n    padding: 15px;\n  }\n\n  .section-title {\n    font-size: 1.3rem;\n  }\n\n  .place-order-button {\n    font-size: 1rem;\n  }\n}\n</style>\n"],"mappings":";;;AA2EA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,cAAc,EAAE,EAAE;QAClBC,OAAO,EAAE,EAAE;QACXC,OAAO,EAAE;MACX;IACF,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,UAAUA,CAAA,EAAG;MACX,MAAMC,IAAG,GAAIC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,WAAW,CAAC,KAAK,EAAE;MAChE,OAAOJ,IAAI,CAACK,MAAM,CAAC,CAACC,GAAG,EAAEC,IAAI,KAAKD,GAAE,GAAIC,IAAI,CAACC,QAAQ,EAAE,CAAC,CAAC;IAC3D,CAAC;IACDC,UAAUA,CAAA,EAAG;MACX,MAAMT,IAAG,GAAIC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,WAAW,CAAC,KAAK,EAAE;MAChE,OAAOJ,IAAG,CACPK,MAAM,CAAC,CAACK,KAAK,EAAEH,IAAI,KAAKG,KAAI,GAAIH,IAAI,CAACI,KAAI,GAAIJ,IAAI,CAACC,QAAQ,EAAE,CAAC,EAC7DI,OAAO,CAAC,CAAC,CAAC;IACf;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,qBAAqBA,CAAA,EAAG;MACtB,IAAI,CAACpB,IAAI,CAACC,cAAa,GAAI,IAAI,CAACD,IAAI,CAACC,cAAc,CAACoB,OAAO,CAAC,SAAS,EAAE,EAAE,CAAC;IAC5E,CAAC;IACD,MAAMC,QAAQA,CAAA,EAAG;MACf,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMzB,KAAK,CAAC0B,IAAI,CAAC,8BAA8B,EAAE;UAChEvB,cAAc,EAAE,IAAI,CAACD,IAAI,CAACC,cAAc;UACxCC,OAAO,EAAE,IAAI,CAACF,IAAI,CAACE,OAAO;UAC1BC,OAAO,EAAE,IAAI,CAACH,IAAI,CAACG,OAAO;UAC1BsB,WAAW,EAAE,CAACC,UAAU,CAAC,IAAI,CAACX,UAAU,IAAI,EAAE,EAAEG,OAAO,CAAC,CAAC;QAC3D,CAAC,CAAC;QAEFS,KAAK,CAACJ,QAAQ,CAACxB,IAAI,CAAC6B,OAAO,CAAC;QAC5B,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,YAAY,CAAC;MACjC,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,