{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      products: [] // List of products to display\n    };\n  },\n  created() {\n    this.fetchProducts();\n  },\n  methods: {\n    async fetchProducts() {\n      try {\n        const response = await axios.get('http://localhost:5555/products');\n        this.products = response.data.products;\n      } catch (error) {\n        console.error('Error fetching products:', error);\n      }\n    },\n    addNewProduct() {\n      // Add logic to navigate to product addition form\n      this.$router.push('/admin/add-product');\n    },\n    editProduct(product) {\n      // Logic to edit product\n      console.log('Edit product:', product);\n    },\n    async deleteProduct(productId) {\n      try {\n        await axios.delete(`http://localhost:5555/products/${productId}`);\n        this.products = this.products.filter(product => product.id !== productId);\n        alert('Product deleted successfully.');\n      } catch (error) {\n        console.error('Error deleting product:', error);\n        alert('Failed to delete product.');\n      }\n    },\n    async handleLogout() {\n      try {\n        await axios.post('http://localhost:5555/logout', {}, {\n          withCredentials: true\n        });\n        localStorage.removeItem('userRole');\n        this.$router.push('/login'); // Redirect to login\n      } catch (error) {\n        console.error('Logout failed', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","products","created","fetchProducts","methods","response","get","error","console","addNewProduct","$router","push","editProduct","product","log","deleteProduct","productId","delete","filter","id","alert","handleLogout","post","withCredentials","localStorage","removeItem"],"sources":["C:\\laragon\\www\\project_appdev\\Frontend\\src\\views\\admin.vue"],"sourcesContent":["<template>\n  <div class=\"container-fluid\">\n    <!-- Sidebar -->\n    <nav class=\"col-md-3 col-lg-2 d-md-block bg-light sidebar\">\n      <div class=\"sidebar-sticky\">\n        <ul class=\"nav flex-column\">\n          <li class=\"nav-item\">\n            <a class=\"nav-link active\" href=\"#\">\n              <i class=\"fas fa-tachometer-alt\"></i>\n              Dashboard\n            </a>\n          </li>\n          <li class=\"nav-item\">\n            <a class=\"nav-link\" href=\"#\">\n              <i class=\"fas fa-box\"></i>\n              Products\n            </a>\n          </li>\n          <li class=\"nav-item\">\n            <a class=\"nav-link\" href=\"#\">\n              <i class=\"fas fa-chart-line\"></i>\n              Insights\n            </a>\n          </li>\n          <li class=\"nav-item\">\n            <a class=\"nav-link\" href=\"#\">\n              <i class=\"fas fa-user-cog\"></i>\n              Settings\n            </a>\n          </li>\n          <li class=\"nav-item\">\n            <a class=\"nav-link text-danger\" @click=\"handleLogout\">\n              <i class=\"fas fa-sign-out-alt\"></i>\n              Logout\n            </a>\n          </li>\n        </ul>\n      </div>\n    </nav>\n\n    <!-- Main Content -->\n    <main class=\"col-md-9 ms-sm-auto col-lg-10 px-4\">\n      <div class=\"d-flex justify-content-between align-items-center pt-3 pb-2 mb-3 border-bottom\">\n        <h1 class=\"h2\">Dashboard</h1>\n        <button class=\"btn btn-primary\" @click=\"addNewProduct\">Add New Product</button>\n      </div>\n\n      <!-- Insights Cards -->\n      <div class=\"row mb-4\">\n        <div class=\"col-md-4\">\n          <div class=\"card border-primary mb-3\">\n            <div class=\"card-header\">Total Sales</div>\n            <div class=\"card-body\">\n              <h5 class=\"card-title\">₱ 500,000</h5>\n              <p class=\"card-text\">Total sales for this month</p>\n            </div>\n          </div>\n        </div>\n        <div class=\"col-md-4\">\n          <div class=\"card border-success mb-3\">\n            <div class=\"card-header\">New Orders</div>\n            <div class=\"card-body\">\n              <h5 class=\"card-title\">50</h5>\n              <p class=\"card-text\">Number of new orders</p>\n            </div>\n          </div>\n        </div>\n        <div class=\"col-md-4\">\n          <div class=\"card border-warning mb-3\">\n            <div class=\"card-header\">Out of Stock</div>\n            <div class=\"card-body\">\n              <h5 class=\"card-title\">10</h5>\n              <p class=\"card-text\">Products currently out of stock</p>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <!-- Product Management Table -->\n      <div class=\"table-responsive\">\n        <h2>Products List</h2>\n        <table class=\"table table-bordered table-hover\">\n          <thead class=\"table-light\">\n            <tr>\n              <th scope=\"col\">#</th>\n              <th scope=\"col\">Product Name</th>\n              <th scope=\"col\">Description</th>\n              <th scope=\"col\">Price</th>\n              <th scope=\"col\">Stock</th>\n              <th scope=\"col\">Actions</th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr v-for=\"(product, index) in products\" :key=\"product.id\">\n              <th scope=\"row\">{{ index + 1 }}</th>\n              <td>{{ product.name }}</td>\n              <td>{{ product.description }}</td>\n              <td>₱ {{ product.price }}</td>\n              <td>{{ product.stock }}</td>\n              <td>\n                <button class=\"btn btn-warning btn-sm\" @click=\"editProduct(product)\">Edit</button>\n                <button class=\"btn btn-danger btn-sm\" @click=\"deleteProduct(product.id)\">Delete</button>\n              </td>\n            </tr>\n          </tbody>\n        </table>\n      </div>\n    </main>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      products: [], // List of products to display\n    };\n  },\n  created() {\n    this.fetchProducts();\n  },\n  methods: {\n    async fetchProducts() {\n      try {\n        const response = await axios.get('http://localhost:5555/products');\n        this.products = response.data.products;\n      } catch (error) {\n        console.error('Error fetching products:', error);\n      }\n    },\n    addNewProduct() {\n      // Add logic to navigate to product addition form\n      this.$router.push('/admin/add-product');\n    },\n    editProduct(product) {\n      // Logic to edit product\n      console.log('Edit product:', product);\n    },\n    async deleteProduct(productId) {\n      try {\n        await axios.delete(`http://localhost:5555/products/${productId}`);\n        this.products = this.products.filter(product => product.id !== productId);\n        alert('Product deleted successfully.');\n      } catch (error) {\n        console.error('Error deleting product:', error);\n        alert('Failed to delete product.');\n      }\n    },\n    async handleLogout() {\n      try {\n        await axios.post('http://localhost:5555/logout', {}, { withCredentials: true });\n        localStorage.removeItem('userRole');\n        this.$router.push('/login'); // Redirect to login\n      } catch (error) {\n        console.error('Logout failed', error);\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n/* Optional: Add any custom styles here */\n.sidebar {\n  background-color: #f8f9fa;\n  height: 100vh;\n  padding: 20px;\n}\n\n.card {\n  margin-bottom: 20px;\n}\n\n.table-hover tbody tr:hover {\n  background-color: #f1f1f1;\n}\n\n.table-bordered th, .table-bordered td {\n  border: 1px solid #ddd;\n}\n</style>\n"],"mappings":";;;AAgHA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE,CAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAA