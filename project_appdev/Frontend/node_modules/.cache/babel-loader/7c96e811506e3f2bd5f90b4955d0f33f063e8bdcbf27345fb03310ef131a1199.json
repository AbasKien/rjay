n}\n\n.cta-button {\n  background-color: #f4511e;\n  color: #fff;\n  padding: 12px 24px;\n  border: none;\n  border-radius: 5px;\n  font-size: 16px;\n  text-decoration: none;\n  display: inline-block;\n  transition: background-color 0.3s ease-in-out;\n}\n\n.cta-button:hover {\n  background-color: #d63b10;\n}\n\n.image img {\n  max-width: 400px;\n  height: auto;\n  border-radius: 10px;\n}\n\n/* Featured Products */\n.featured-products {\n  text-align: center;\n  padding: 50px 20px;\n  background-color: #fff;\n  border-radius: 8px;\n  box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.1);\n  margin: 30px auto;\n  max-width: 1200px;\n}\n\n.featured-products h2 {\n  font-size: 28px;\n  color: #333;\n  margin-bottom: 30px;\n}\n\n.product-list {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));\n  gap: 20px;\n}\n\n.product-card {\n  background: #fff;\n  border: 1px solid #eee;\n  border-radius: 8px;\n  padding: 15px;\n  text-align: center;\n  box-shadow: 0px 2px 4px rgba(0, 0, 0, 0.1);\n  transition: transform 0.2s;\n}\n\n.product-card:hover {\n  transform: translateY(-5px);\n}\n\n.product-image {\n  max-width: 100%;\n  height: auto;\n  margin-bottom: 15px;\n}\n\n.product-title {\n  font-size: 16px;\n  font-weight: bold;\n  color: #333;\n}\n\n.product-price {\n  color: #f4511e;\n  font-weight: bold;\n  margin: 10px 0;\n}\n\n.add-to-cart {\n  background-color: #f4511e;\n  color: #fff;\n  border: none;\n  padding: 10px 20px;\n  border-radius: 5px;\n  font-size: 14px;\n  cursor: pointer;\n  transition: background-color 0.3s ease-in-out;\n}\n\n.add-to-cart:hover {\n  background-color: #d63b10;\n}\n\n/* Footer */\n.footer {\n  background-color: #333;\n  color: #fff;\n  text-align: center;\n  padding: 20px 0;\n}\n\n.footer a {\n  color: #f4511e;\n  text-decoration: none;\n}\n</style>\n"],"mappings":";AAyEA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,UAAU;EAEhBC,OAAO,EAAE;IACP,MAAMC,YAAYA,CAACC,KAAK,EAAE;MACxBA,KAAK,CAACC,cAAc,CAAC,CAAC,EAAG;MACzB,IAAI;