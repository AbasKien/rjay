{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.reduce.js\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      form: {\n        contact_number: \"\",\n        address: \"\",\n        payment: \"\"\n      }\n    };\n  },\n  computed: {\n    totalItems() {\n      const cart = JSON.parse(localStorage.getItem(\"cartItems\")) || [];\n      return cart.reduce((sum, item) => sum + item.quantity, 0);\n    },\n    totalPrice() {\n      const cart = JSON.parse(localStorage.getItem(\"cartItems\")) || [];\n      return cart.reduce((total, item) => total + item.price * item.quantity, 0).toFixed(2);\n    }\n  },\n  methods: {\n    validateContactNumber() {\n      this.form.contact_number = this.form.contact_number.replace(/[^0-9]/g, \"\");\n    },\n    async checkout() {\n      try {\n        const response = await axios.post(\"http://localhost:5555/orders\", {\n          contact_number: this.form.contact_number,\n          address: this.form.address,\n          payment: this.form.payment,\n          total_price: (parseFloat(this.totalPrice) + 58).toFixed(2)\n        });\n        alert(response.data.message);\n        this.$router.push(\"/thank-you\");\n      } catch (error) {\n        console.error(\"Failed to checkout:\", error);\n        alert(\"There was an error placing your order. Please try again.\");\n      }\n    },\n    goToCart() {\n      this.$router.push(\"/cart\");\n    }\n  }\n};","map":{"version":3,"names":["axios","data","form","contact_number","address","payment","computed","totalItems","cart","JSON","parse","localStorage","getItem","reduce","sum","item","quantity","totalPrice","total","price","toFixed","methods","validateContactNumber","replace","checkout","response","post","total_price","parseFloat","alert","message","$router","push","error","console","goToCart"],"sources":["C:\\laragon\\www\\project_appdev\\Frontend\\src\\views\\checkout.vue"],"sourcesContent":["<template>\n  <div class=\"container my-5\">\n    <h1 class=\"text-center mb-4\">Checkout</h1>\n    <div class=\"row g-4\">\n      <!-- Left Column: Personal Information -->\n      <div class=\"col-md-6\">\n        <div class=\"card shadow-sm border-0\">\n          <div class=\"card-header bg-primary text-white\">\n            <h3 class=\"mb-0\">Personal Information</h3>\n          </div>\n          <div class=\"card-body\">\n            <form @submit.prevent=\"checkout\" class=\"needs-validation\" novalidate>\n              <div class=\"mb-3\">\n                <label for=\"contact-number\" class=\"form-label\">Contact Number</label>\n                <input\n                  type=\"tel\"\n                  id=\"contact-number\"\n                  class=\"form-control\"\n                  v-model=\"form.contact_number\"\n                  required\n                  placeholder=\"Enter your contact number\"\n                  @input=\"validateContactNumber\"\n                  maxlength=\"11\"\n                />\n                <div class=\"invalid-feedback\">Please enter a valid contact number.</div>\n              </div>\n              <div class=\"mb-3\">\n                <label for=\"address\" class=\"form-label\">Shipping Address</label>\n                <textarea\n                  id=\"address\"\n                  class=\"form-control\"\n                  v-model=\"form.address\"\n                  required\n                  placeholder=\"Enter your shipping address\"\n                ></textarea>\n                <div class=\"invalid-feedback\">Please enter your address.</div>\n              </div>\n              <div class=\"mb-3\">\n                <label for=\"payment\" class=\"form-label\">Payment Method</label>\n                <select v-model=\"form.payment\" id=\"payment\" class=\"form-select\" required>\n                  <option value=\"check_payment\">Check Payment</option>\n                  <option value=\"cod\">Cash on Delivery (COD)</option>\n                </select>\n                <div class=\"invalid-feedback\">Please select a payment method.</div>\n              </div>\n              <!-- Centered Back to Cart Button -->\n              <div class=\"d-flex justify-content-center mt-3\">\n                <button type=\"button\" class=\"btn btn-secondary w-100\" @click=\"goToCart\">Back to Cart</button>\n              </div>\n            </form>\n          </div>\n        </div>\n      </div>\n\n      <!-- Right Column: Order Summary -->\n      <div class=\"col-md-6\">\n        <div class=\"card shadow-sm border-0\">\n          <div class=\"card-header bg-info text-white\">\n            <h3 class=\"mb-0\">Order Summary</h3>\n          </div>\n          <div class=\"card-body\">\n            <ul class=\"list-group\">\n              <li class=\"list-group-item d-flex justify-content-between\">\n                <span>Total Items:</span>\n                <span>{{ totalItems }}</span>\n              </li>\n              <li class=\"list-group-item d-flex justify-content-between\">\n                <span>Total Price:</span>\n                <span>₱{{ totalPrice }}</span>\n              </li>\n              <li class=\"list-group-item d-flex justify-content-between\">\n                <span>Shipping:</span>\n                <span>₱58.00</span>\n              </li>\n              <li class=\"list-group-item d-flex justify-content-between fw-bold\">\n                <span>Total Amount:</span>\n                <span>₱{{ (parseFloat(totalPrice) + 58).toFixed(2) }}</span>\n              </li>\n            </ul>\n            <div class=\"d-flex justify-content-center mt-3\">\n              <button type=\"submit\" class=\"btn btn-success w-100 py-2\">\n                Place Order\n              </button>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  data() {\n    return {\n      form: {\n        contact_number: \"\",\n        address: \"\",\n        payment: \"\",\n      },\n    };\n  },\n  computed: {\n    totalItems() {\n      const cart = JSON.parse(localStorage.getItem(\"cartItems\")) || [];\n      return cart.reduce((sum, item) => sum + item.quantity, 0);\n    },\n    totalPrice() {\n      const cart = JSON.parse(localStorage.getItem(\"cartItems\")) || [];\n      return cart\n        .reduce((total, item) => total + item.price * item.quantity, 0)\n        .toFixed(2);\n    },\n  },\n  methods: {\n    validateContactNumber() {\n      this.form.contact_number = this.form.contact_number.replace(/[^0-9]/g, \"\");\n    },\n    async checkout() {\n      try {\n        const response = await axios.post(\"http://localhost:5555/orders\", {\n          contact_number: this.form.contact_number,\n          address: this.form.address,\n          payment: this.form.payment,\n          total_price: (parseFloat(this.totalPrice) + 58).toFixed(2),\n        });\n\n        alert(response.data.message);\n        this.$router.push(\"/thank-you\");\n      } catch (error) {\n        console.error(\"Failed to checkout:\", error);\n        alert(\"There was an error placing your order. Please try again.\");\n      }\n    },\n    goToCart() {\n      this.$router.push(\"/cart\");\n    }\n  },\n};\n</script>\n\n<style scoped>\n/* Modern E-commerce Styling */\n.container {\n  min-height: 100vh;\n  max-width: 1400px;\n  margin: 0 auto;\n  padding: 2rem 5%;\n  font-family: \"Poppins\", sans-serif;\n  background: linear-gradient(135deg, #f5f7fa 0%, #e4e8f0 100%);\n  color: #2d3748;\n}\n\nh1 {\n  font-size: 2rem;\n  font-weight: 700;\n  color: #1a202c;\n  text-align: center;\n  margin-bottom: 2rem;\n  position: relative;\n}\n\nh1::after {\n  content: '';\n  position: absolute;\n  bottom: -10px;\n  left: 50%;\n  transform: translateX(-50%);\n  width: 60px;\n  height: 4px;\n  background: linear-gradient(90deg, #4f46e5, #7c3aed);\n  border-radius: 2px;\n}\n\n.row {\n  display: flex;\n  gap: 2rem;\n  margin: 2rem 0;\n}\n\n@media (max-width: 768px) {\n  .row {\n    flex-direction: column;\n  }\n}\n\n/* Card Styling */\n.card {\n  background: white;\n  border-radius: 16px;\n  box-shadow: 0 4px 6px -1px rgba(0, 0, 0, 0.1);\n  overflow: hidden;\n  position: relative;\n}\n\n.card::before {\n  content: '';\n  position: absolute;\n  top: 0;\n  left: 0;\n  right: 0;\n  height: 4px;\n  background: linear-gradient(90deg, #4f46e5, #7c3aed);\n}\n\n.card-header {\n  background: white;\n  border: none;\n  padding: 1.5rem;\n}\n\n.card-header h3 {\n  font-size: 1.5rem;\n  font-weight: 700;\n  color: #1a202c;\n  margin: 0;\n}\n\n.card-body {\n  padding: 1.5rem;\n}\n\n/* Form Styling */\n.form-label {\n  font-weight: 600;\n  color: #4b5563;\n  margin-bottom: 0.5rem;\n}\n\n.form-control, .form-select {\n  width: 100%;\n  padding: 0.75rem;\n  border: 2px solid #e5e7eb;\n  border-radius: 8px;\n  font-size: 0.95rem;\n  color: #4b5563;\n  transition: all 0.3s ease;\n}\n\n.form-control:focus, .form-select:focus {\n  border-color: #4f46e5;\n  box-shadow: 0 0 0 3px rgba(79, 70, 229, 0.1);\n  outline: none;\n}\n\ntextarea.form-control {\n  min-height: 100px;\n  resize: vertical;\n}\n\n.invalid-feedback {\n  color: #ef4444;\n  font-size: 0.875rem;\n  margin-top: 0.25rem;\n}\n\n/* List Group Styling */\n.list-group {\n  margin: 1rem 0;\n}\n\n.list-group-item {\n  display: flex;\n  justify-content: space-between;\n  padding: 1rem;\n  border: none;\n  color: #4b5563;\n}\n\n.list-group-item:not(:last-child) {\n  border-bottom: 1px solid #e5e7eb;\n}\n\n.list-group-item.fw-bold {\n  color: #1a202c;\n  font-weight: 600;\n  font-size: 1.1rem;\n}\n\n/* Button Styling */\n.btn {\n  display: inline-block;\n  padding: 0.75rem 1.5rem;\n  border: none;\n  border-radius: 8px;\n  font-weight: 600;\n  cursor: pointer;\n  transition: all 0.3s ease;\n  width: 100%;\n  text-align: center;\n}\n\n.btn-secondary {\n  background: #f3f4f6;\n  color: #4b5563;\n}\n\n.btn-secondary:hover {\n  background: #e5e7eb;\n  color: #1a202c;\n  transform: translateY(-2px);\n}\n\n.btn-success {\n  background: linear-gradient(135deg, #4f46e5 0%, #7c3aed 100%);\n  color: white;\n}\n\n.btn-success:hover {\n  transform: translateY(-2px);\n  box-shadow: 0 4px 6px -1px rgba(79, 70, 229, 0.2);\n}\n\n.btn:active {\n  transform: translateY(0);\n}\n\n/* Responsive Design */\n@media (max-width: 768px) {\n  .container {\n    padding: 1rem;\n  }\n\n  .card {\n    margin-bottom: 1rem;\n  }\n\n  .btn {\n    padding: 0.75rem 1rem;\n  }\n}\n\n@media (max-width: 480px) {\n  h1 {\n    font-size: 1.75rem;\n  }\n\n  .card-header h3 {\n    font-size: 1.25rem;\n  }\n\n  .form-control, .form-select {\n    font-size: 0.9rem;\n  }\n}\n</style>\n"],"mappings":";;;AA4FA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,cAAc,EAAE,EAAE;QAClBC,OAAO,EAAE,EAAE;QACXC,OAAO,EAAE;MACX;IACF,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,UAAUA,CAAA,EAAG;MACX,MAAMC,IAAG,GAAIC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,WAAW,CAAC,KAAK,EAAE;MAChE,OAAOJ,IAAI,CAACK,MAAM,CAAC,CAACC,GAAG,EAAEC,IAAI,KAAKD,GAAE,GAAIC,IAAI,CAACC,QAAQ,EAAE,CAAC,CAAC;IAC3D,CAAC;IACDC,UAAUA,CAAA,EAAG;MACX,MAAMT,IAAG,GAAIC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,WAAW,CAAC,KAAK,EAAE;MAChE,OAAOJ,IAAG,CACPK,MAAM,CAAC,CAACK,KAAK,EAAEH,IAAI,KAAKG,KAAI,GAAIH,IAAI,CAACI,KAAI,GAAIJ,IAAI,CAACC,QAAQ,EAAE,CAAC,EAC7DI,OAAO,CAAC,CAAC,CAAC;IACf;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,qBAAqBA,CAAA,EAAG;MACtB,IAAI,CAACpB,IAAI,CAACC,cAAa,GAAI,IAAI,CAACD,IAAI,CAACC,cAAc,CAACoB,OAAO,CAAC,SAAS,EAAE,EAAE,CAAC;IAC5E,CAAC;IACD,MAAMC,QAAQA,CAAA,EAAG;MACf,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMzB,KAAK,CAAC0B,IAAI,CAAC,8BAA8B,EAAE;UAChEvB,cAAc,EAAE,IAAI,CAACD,IAAI,CAACC,cAAc;UACxCC,OAAO,EAAE,IAAI,CAACF,IAAI,CAACE,OAAO;UAC1BC,OAAO,EAAE,IAAI,CAACH,IAAI,CAACG,OAAO;UAC1BsB,WAAW,EAAE,CAACC,UAAU,CAAC,IAAI,CAACX,UAAU,IAAI,EAAE,EAAEG,OAAO,CAAC,CAAC;QAC3D,CAAC,CAAC;QAEFS,KAAK,CAACJ,QAAQ,CAACxB,IAAI,CAAC6B,OAAO,CAAC